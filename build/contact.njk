---
title: Contact Jaycee
layout: 'base.njk'
---

<section class="contact-section">
	<div class="img-container">
		<div class="img-box"></div>
		<div class="img-box"></div>
		<div class="img-box"></div>

		<div class="text-container">
			<h3 class="title">Contact <span class="highlight">Jaycee</span></h3>

			<div class="contact-details">
				<div class="wrap">
					<i class="fa-solid fa-envelope"></i>
					<p>hello@jayceetalks.com</p>
				</div>

				<div class="wrap">
					<i class="fa-solid fa-envelope"></i>
					<p>hello@jayceetalks.com</p>
				</div>
			</div>

			<div class="social-media">
				<i class="fa-brands fa-x-twitter"></i>
				<i class="fa-brands fa-instagram"></i>
				<i class="fa-brands fa-facebook"></i>
			</div>
		</div>
	</div>
</section>

<section class="form-section">
	<div class="container">
		<p class="text">
			Knowing the reason for contacting us helps us respond accordingly and
			appropriately
		</p>

		<form action="" class="form">
			<div class="input-wrap">
				<input type="text" placeholder="Name" class="input" />
			</div>

			<div class="input-wrap">
				<input type="email" placeholder="Email" class="input" />
			</div>

			<div class="input-wrap">
				<input type="text" placeholder="Phone" class="input" />
			</div>

			<div class="message-wrap">
				<textarea name="message" placeholder="Message" id=""></textarea>
			</div>

			<button class="btn">Submit Message</button>
		</form>
	</div>
</section>
